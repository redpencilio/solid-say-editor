{{#if (isequal this.currentScreen this.screens.FILES)}}
<Selectors::FileSelector  @onSelect={{this.setSelectedFile}} @selectedFile={{this.selectedFile}} @onClose={{this.onClose}} @onOpen={{action (mut this.currentScreen) this.screens.SUBJECTS}}/>
{{else if (isequal this.currentScreen this.screens.SUBJECTS)}}
<Selectors::SubjectSelector @onSelect={{action (mut this.selectedSubject)}} @selectedSubject={{this.selectedSubject}} @file={{this.selectedGraph}} @onClose={{this.onClose}} @onOpen={{action (mut this.currentScreen) this.screens.PREDICATES}}/>
{{else}}
<Selectors::PredicateSelector @onClose={{this.onClose}} @subject={{this.selectedSubject}} @graph={{this.selectedGraph}}/>
{{/if}}
